{% extends 'base-front.html.twig' %}

{% block body %}

<div class="container">
   {{form_start(form)}}
        <div class="my-3">{{form_row(form.password)}}</div>
        <div class="my-3">{{form_row(form.password_confirmation)}}</div>
        <button class="btn btn-primary">Valider</button>
   {{form_end(form)}}

    {% if error is defined %}
        <p class="my-3">{{error}}</p>
    {% endif %}
</div>

{% endblock %}